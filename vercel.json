{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "functions": {
      "api/*.py": {
        "memory": 1024,
        "maxDuration": 10
      }
    },
    "ignore": [
      "firebase_key.json",
      "data/",
      "notebooks/",
      "*.pkl",
      "tests/",
      ".git/"
    ]
  }
  